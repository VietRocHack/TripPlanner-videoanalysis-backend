[Unit]
Description=Gunicorn instance for TripPlanner video analysis backend, development server
After=network.target

[Service]
User=ec2-user
Group=nobody
WorkingDirectory=/home/ec2-user/tripplanner-backend/videoanalysis/dev/src
ExecStart=/home/ec2-user/tripplanner-backend/videoanalysis/dev/src/venv/bin/gunicorn -w 4 --timeout 120 -b 0.0.0.0:8003 wsgi:app 
Restart=always

[Install]
WantedBy=multi-user.target